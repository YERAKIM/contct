<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app.contactapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.MemberActivity" />
        <activity android:name=".presentation.SginupActivity"></activity>
        <activity android:name=".presentation.SgininActivity" />
        <activity android:name=".presentation.DetailActivity" />
        <activity android:name=".presentation.UpdateActivity" />
        <activity android:name=".presentation.ListActivity" />
        <activity android:name=".presentation.MypageActivity" />
        <activity android:name=".presentation.scheduleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>